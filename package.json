{
  "homepage": "http://kenneth-andales.github.io/libre",
  "name": "libre-app-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "tailwindcss": "concurrently \"npm run dev\" \"npm run watch:tailwindcss\"",
    "watch:tailwindcss": "npx tailwindcss -i ./src/tailwind.css -o ./src/tailwindstyles.css --watch",
    "build": "vite build",
    "preview": "vite preview",
    "sync": "aws s3 sync ./dist s3://libre.centapps.online/"
  },
  "dependencies": {
    "hamburger-react": "^2.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-spinners": "^0.13.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "gh-pages": "^5.0.0",
    "tailwindcss": "^3.1.7",
    "vite": "^3.0.0"
  }
}
